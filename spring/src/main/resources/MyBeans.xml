<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context/
		http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="myBean"  class="com.akagiyui.MyBean" />

	<bean id="staticFactoryBean" class="com.akagiyui.StaticFactory" factory-method="createStaticFactoryBean" />

	<bean id="instanceFactory" class="com.akagiyui.InstanceFactory" />
	<bean id="instanceFactoryBean" factory-bean="instanceFactory" factory-method="createInstanceFactoryBean" />

	<bean id="constructInjectBean" class="com.akagiyui.ConstructInjectBean">
		<constructor-arg name="name" value="akagiyui" />
	</bean>

	<bean id="setterInjectBean" class="com.akagiyui.SetterInjectBean">
		<property name="name" value="akagiyui" />
	</bean>

	<bean id="singletonBean" class="com.akagiyui.SingletonBean" scope="singleton" />
	<bean id="prototypeBean" class="com.akagiyui.PrototypeBean" scope="prototype" />
</beans>
